{
  "name": "albumartcreator",
  "productName": "Album Art Creator",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "dependencies": {
    "console-browserify": "^1.2.0",
    "electron": "^9.1.0",
    "html-to-image": "^0.1.1",
    "save-html-as-image": "^1.3.3"
  },
  "devDependencies": {},
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/icon.icns --prune=true --out=release-builds --aser=true",
    "package-win": "electron-packager . albumartcreator --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Album Art Creator\"",    
    "package-linux": "electron-packager . albumartcreator --overwrite --asar=true --platform=linux --arch=x64 --icon=icons/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/0neGuyDev/albumArtCreator.git"
  },
  "author": "0neGuyDev",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/0neGuyDev/albumArtCreator/issues"
  },
  "homepage": "https://github.com/0neGuyDev/albumArtCreator#readme"
}
